@html.extend('layout', function(model){
	@html.block('content', function(model){
		<p>This is from the vash engine!</p>
		 <div data-ng-controller="screen1Controller">
			<div>{{class}}</div>
			<div><input type="text" data-ng-model="class"/></div>
		 </div>
		 <div data-ng-controller="screen2Controller">
			 <div>{{name}}</div>
			 <div><input type="text" data-ng-model="name"/></div>
		 </div>
	})
})